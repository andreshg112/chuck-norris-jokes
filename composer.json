{
  "name": "andreshg112/chuck-norris-jokes",
  "description": "Muestra chistes aleatorios sobre Chuck Norris.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrés Herrera García",
      "email": "andreshg112@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Andreshg112\\ChuckNorrisJokes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Andreshg112\\ChuckNorrisJokes\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "~3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Andreshg112\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
      ],
      "aliases": {
        "ChuckNorris": "Andreshg112\\ChuckNorrisJokes\\Facades\\ChuckNorris"
      }
    }
  }
}
